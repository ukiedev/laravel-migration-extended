{
    "name": "ukiedev/laravel-migration-extended",
    "description": "Extends default migration command for migrating specific or first migration",
    "type": "library",
    "require": {
        "laravel/framework": "^5.5"
    },
    "authors": [
        {
            "name": "dorian",
            "email": "d.melnyk@tenantcloud.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "App\\MigrationExtendedProvider"
            ]
        }
    }
}
